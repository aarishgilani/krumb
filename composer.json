{
  "name": "aarishgilani/krumb",
  "type": "package",
  "license": "MIT",
  "description": "A simple WordPress breadcrumb for Sage 10.",
  "authors": [
    {
      "name": "Brandon Nifong",
      "email": "brandon@tendency.me"
    },
    {
      "name": "Aarish Gilani",
      "email": "aarishgilani10@gmail.com"
    }
  ],
  "keywords": [
    "wordpress",
    "sage",
    "roots",
    "breadcrumb"
  ],
  "support": {
    "issues": "https://github.com/aarishgilani/krumb/issues"
  },
  "autoload": {
    "psr-4": {
      "Aarishgilani\\Krumb\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "extra": {
    "acorn": {
      "providers": [
        "Aarishgilani\\Krumb\\KrumbServiceProvider"
      ],
      "aliases": {
        "Krumb": "Aarishgilani\\Krumb\\Facades\\Krumb"
      }
    }
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor --extensions=php --standard=PSR12 ."
    ]
  }
}
